RewriteEngine On
RewriteCond %{SERVER_PORT} 80
RewriteCond %{HTTP_HOST} ^schoolofinteriorredesign.com$ [OR]
RewriteCond %{HTTP_HOST} ^www.schoolofinteriorredesign.com$
RewriteRule ^(.*)$ https://schoolofinteriorredesign.com/$1 [R,L]

Options -Indexes

ErrorDocument 404 /404.htm
ErrorDocument 403 /404.htm
